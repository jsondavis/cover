{
  "name": "jsondavis/cover",
  "type": "project",
  "version": "0.0.1",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "JSONDAVIS\\Cover\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JSONDAVIS\\Tests\\Cover\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "jsondavis"
    }
  ],
  "require": {
    "php": "~8.4",
    "ext-json": "*",
    "doctrine/common": "^3.5",
    "doctrine/dbal": "^4.0",
    "doctrine/migrations": "^3.9",
    "doctrine/orm": "^3.6",
    "fakerphp/faker": "^1.23",
    "slim/php-view": "^3.4",
    "slim/psr7": "^1.8",
    "slim/slim": "4.*",
    "symfony/cache": "^8.0",
    "symfony/var-exporter": "7.4",
    "uma/dic": "^4.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "roave/security-advisories": "dev-master"
  },
  "scripts": {
    "setup": [
      "@composer install",
      "./bin/doc orm:schema-tool:update --force"
    ],
    "test": [
      "php vendor/bin/phpunit"
    ]
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
