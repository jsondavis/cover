{
    "name": "jsondavis/cover",
    "type": "project",
    "version": "0.0.1",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jsondavis\\Cover\\": "src/"
        }
    },
    "authors": [
        {
            "name": "jsondavis"
        }
    ],
    "require": {
        "php": "~8.4",
        "ext-json": "*",
        "slim/slim": "4.*",
        "doctrine/dbal": "^4.0",
        "doctrine/orm": "^3.6",
        "doctrine/common": "^3.5",
        "doctrine/migrations": "^3.9",
        "slim/psr7": "^1.8",
        "symfony/cache": "^8.0",
        "vlucas/phpdotenv": "^5.6",
        "symfony/var-exporter": "7.4",
        "slim/php-view": "^3.4",
        "uma/dic": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "roave/security-advisories": "dev-master"
    },
    "scripts": {
      "setup": [
        "@composer install",
        "./bin/doc orm:schema-tool:update --force"
      ],
      "test": [
        "php vendor/bin/phpunit"
      ]
    }
}
